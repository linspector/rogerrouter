<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkImage" id="image1">
    <property name="icon_name">document-edit-symbolic</property>
  </object>
  <template class="RogerPreferencesWindow" parent="AdwPreferencesWindow">
    <property name="default_height">700</property>
    <property name="default_width">640</property>
    <property name="width_request">300</property>
    <property name="modal">True</property>
    <child>
      <object class="AdwPreferencesPage">
        <property name="icon_name">preferences-system-network-symbolic</property>
        <property name="title" translatable="yes">Connections</property>
        <child>
          <object class="AdwPreferencesGroup">
            <property name="title" translatable="yes">Router</property>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">Host</property>
                <property name="activatable">False</property>
                <child>
                  <object class="GtkEntry" id="host">
                    <property name="valign">center</property>
                    <property name="text"></property>
                    <property name="xalign">1.0</property>
                    <property name="has-frame">False</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">User</property>
                <property name="activatable">False</property>
                <child>
                  <object class="GtkEntry" id="login_user">
                    <property name="valign">center</property>
                    <property name="text"></property>
                    <property name="xalign">1.0</property>
                    <property name="has-frame">False</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">Password</property>
                <property name="activatable">False</property>
                <child>
                  <object class="GtkEntry" id="login_password">
                    <property name="valign">center</property>
                    <property name="text"></property>
                    <property name="visibility">False</property>
                    <property name="xalign">1.0</property>
                    <property name="has-frame">False</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwPreferencesGroup">
            <property name="title" translatable="yes">Codes</property>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">External</property>
                <property name="activatable">False</property>
                <child>
                  <object class="GtkEntry" id="external_code">
                    <property name="valign">center</property>
                    <property name="text"></property>
                    <property name="xalign">1.0</property>
                    <property name="has-frame">False</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">International</property>
                <property name="activatable">False</property>
                <child>
                  <object class="GtkEntry" id="international_code">
                    <property name="valign">center</property>
                    <property name="text"></property>
                    <property name="xalign">1.0</property>
                    <property name="has-frame">False</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">Country</property>
                <property name="activatable">False</property>
                <child>
                  <object class="GtkEntry" id="country_code">
                    <property name="valign">center</property>
                    <property name="text"></property>
                    <property name="xalign">1.0</property>
                    <property name="has-frame">False</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">National</property>
                <property name="activatable">False</property>
                <child>
                  <object class="GtkEntry" id="national_code">
                    <property name="valign">center</property>
                    <property name="text"></property>
                    <property name="xalign">1.0</property>
                    <property name="has-frame">False</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">Area</property>
                <property name="activatable">False</property>
                <child>
                  <object class="GtkEntry" id="area_code">
                    <property name="valign">center</property>
                    <property name="text"></property>
                    <property name="xalign">1.0</property>
                    <property name="has-frame">False</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwPreferencesGroup">
            <property name="title" translatable="yes">Devices</property>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">Softphone</property>
                <property name="subtitle" translatable="yes">Enable softphone support</property>
                <child>
                  <object class="GtkSwitch" id="softphone">
                    <property name="valign">center</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">Softfax</property>
                <property name="subtitle" translatable="yes">Enable softfax support</property>
                <child>
                  <object class="GtkSwitch" id="softfax">
                    <property name="valign">center</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwPreferencesGroup" id="softphone_group">
            <property name="title" translatable="yes">Softphone</property>
            <child>
              <object class="AdwComboRow" id="softphone_number">
                <property name="title" translatable="yes">Number</property>
                <property name="subtitle" translatable="yes">Phone number which will be used by softphone</property>
              </object>
            </child>
            <child>
              <object class="AdwComboRow" id="softphone_controller">
                <property name="title" translatable="yes">Controller</property>
                <property name="subtitle" translatable="yes">Router controller to take of router connections</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwPreferencesGroup" id="softfax_group">
            <property name="title" translatable="yes">Softfax</property>
            <child>
              <object class="AdwComboRow" id="softfax_number">
                <property name="title" translatable="yes">Number</property>
              </object>
            </child>
            <child>
              <object class="AdwComboRow" id="softfax_controller">
                <property name="title" translatable="yes">Controller</property>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">Header</property>
                <property name="activatable">False</property>
                <child>
                  <object class="GtkEntry" id="softfax_header">
                    <property name="valign">center</property>
                    <property name="text"></property>
                    <property name="xalign">1.0</property>
                    <property name="has-frame">False</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">Ident</property>
                <property name="activatable">False</property>
                <child>
                  <object class="GtkEntry" id="softfax_ident">
                    <property name="valign">center</property>
                    <property name="text"></property>
                    <property name="xalign">1.0</property>
                    <property name="has-frame">False</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwComboRow" id="softfax_resolution">
                <property name="title" translatable="yes">Resolution</property>
              </object>
            </child>
            <child>
              <object class="AdwComboRow" id="softfax_service">
                <property name="title" translatable="yes">Service</property>
              </object>
            </child>
            <child>
              <object class="AdwExpanderRow" id="softfax_report">
                <property name="title" translatable="yes">Fax reports</property>
                <property name="show_enable_switch">True</property>
                <child>
                  <object class="AdwActionRow">
                    <property name="title" translatable="yes">Directory</property>
                    <child>
                      <object class="GtkButton" id="softfax_directory">
                        <property name="valign">center</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">Error correction</property>
                <child>
                  <object class="GtkSwitch" id="softfax_ecm">
                    <property name="valign">center</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="AdwPreferencesPage">
        <property name="icon_name">view-list-symbolic</property>
        <property name="title" translatable="yes">Journal</property>
        <child>
          <object class="AdwPreferencesGroup" id="notification_incoming">
            <property name="title" translatable="yes">Incoming Notifications</property>
          </object>
        </child>
        <child>
          <object class="AdwPreferencesGroup" id="notification_outgoing">
            <property name="title" translatable="yes">Outgoing Notifications</property>
          </object>
        </child>
        <child>
          <object class="AdwPreferencesGroup" id="filter">
            <property name="title" translatable="yes">Filter</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="AdwPreferencesPage">
        <property name="icon_name">audio-headset-symbolic</property>
        <property name="title" translatable="yes">Audio</property>
        <child>
          <object class="AdwPreferencesGroup">
            <property name="title" translatable="yes">Configuration</property>
            <child>
              <object class="AdwActionRow">
                <property name="title" translatable="yes">Ringtone</property>
                <child>
                  <object class="GtkSwitch" id="ringtone">
                    <property name="valign">center</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwPreferencesGroup">
            <property name="title" translatable="yes">Devices</property>
            <child>
              <object class="AdwComboRow" id="microphone">
                <property name="title" translatable="yes">Microphone</property>
              </object>
            </child>
            <child>
              <object class="AdwComboRow" id="speaker">
                <property name="title" translatable="yes">Speaker</property>
              </object>
            </child>
            <child>
              <object class="AdwComboRow" id="ringer">
                <property name="title" translatable="yes">Ringer</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="AdwPreferencesPage">
        <property name="icon_name">application-x-addon-symbolic</property>
        <property name="title" translatable="yes">Plugins</property>
        <child>
          <object class="AdwPreferencesGroup">
            <property name="title" translatable="yes">Configuration</property>
            <child>
              <object class="GtkListBox" id="plugins_listbox">
                <property name="selection-mode">none</property>
                <style>
                  <class name="content"/>
                </style>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>

